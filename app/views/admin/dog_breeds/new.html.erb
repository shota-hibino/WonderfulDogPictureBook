<% content_for :title do %>
  ワンだふる犬図鑑-犬種新規登録
<% end %>

<div class="container">
  <h2 class="dog_breed-new-edit-title">新規犬種登録</h2>
  <div class="dog_breed_new_edit">
    <%= form_with model:@dog_breed, url: admin_dog_breeds_path, local:true, method: :post, class: "form-horizontal" do |f| %>
      <%= render "layouts/error_messages", model: f.object, resource: @dog_breed %>
      <%= render "form_new_dog_breed", f: f %>


    <div class="col text-center">
      <%= f.submit "新規登録", class: "btn btn-success btn-lg text-center" %>
    </div>
    <% end %>
  </div>
</div>
